{
  "name": "@cohubinc/cohub-ui",
  "version": "0.1.72",
  "description": "Sharable UI components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Cohub",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development rollup -cw",
    "build": "NODE_ENV=production rollup -c",
    "build:stories": "NODE_ENV=storybook rollup -c",
    "watch:stories": "NODE_ENV=storybook rollup -cw",
    "storybook": "start-storybook -p 6007",
    "storybook:build": "yarn build:stories && build-storybook -c .storybook -o .storybook/stories-dist",
    "pre-push-msg": "echo 'Pre-push checks...' && exit 0",
    "pre-push-test": "yarn test --no-watch",
    "lint": "tslint --project tsconfig.build.json",
    "lint-fix": "tslint --fix --project tsconfig.build.json",
    "test": "NODE_ENV=testing yarn checkTypes && yarn jest",
    "test-build": "NODE_ENV=testing yarn build && yarn test",
    "test:watch": "NODE_ENV=testing yarn test --watch ",
    "test:coverage": "yarn test --coverage",
    "checkTypes": "yarn tsc --noEmit",
    "publish:npm": "yarn build && yarn publish"
  },
  "lint-staged": {
    "./src/*.{ts,tsx}": [
      "tslint --fix --project tsconfig.build.json",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push-msg": "echo 'Building dist. You will likely need to amend your previous commit and force push the changes to dist.' && exit 0",
      "pre-push": "yarn storybook:build && yarn test-build --no-watch"
    }
  },
  "dependencies": {
    "@sentry/browser": "5.6.1",
